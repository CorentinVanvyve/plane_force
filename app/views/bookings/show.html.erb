

<div class = "page-body-bookings">
<div id = "bookings-container">
  <div class = "booking-card">
    <% if @booking.plane.photos.attached?%>
  <div class = "booking-card-img" style ="background-image:
    url('<%= cl_image_path @booking.plane.photos[0].key%>')">
  <% else%>
    <div class= "booking-card-img" style = "background-image: url('https://www.arachnys.com/wp-content/uploads/2022/02/iStock-466619660.jpg');">
  <%end%>
  </div>
    <div class = "booking-card-header">
      <div class = "avatar">
        <%= render "shared/avatar"%> 
            </div>
            <p><%=@booking.user.first_name.capitalize%> <%=@booking.user.last_name.capitalize%>
      </div>
            <div class = "booking-stats">
<p>Model</p><%=@booking.plane.model%>
<p>Plane Name</p><%=@booking.plane.name%>
<p>Start Date</p> <%=@booking.start_date%>
<p>End Date</p> <%=@booking.end_date%>
<p>Price Per Hour</p><%=@booking.plane.price_per_hour%>

<p>Renter</p><%=@booking.user.first_name%> <%=@booking.user.last_name%>


</div>
             
            
        </div>
    </div>
</div>



